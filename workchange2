import xlrd
import xlwt

def read_excel(file_path,sheet_name):
    workbook = xlrd.open_workbook(filepath)
    sheet_names = workbook.sheet_names()
    if sheet_name in sheet_names:
        sheet = workbook.sheet_by_name(sheet_name)
        for i in range(sheet.nrows):
            yield(sheet.row(i))
